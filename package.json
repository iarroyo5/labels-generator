  "author": "Inaki Arrroyo <yo@inakiarroyo.com> (http://inakiarroyo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iarroyo5/labels-generator/issues"
  },
  "homepage": "https://github.com/iarroyo5/labels-generator#readme",
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^2.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/uuid": "^3.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.5.7",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
