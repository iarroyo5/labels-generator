{
  "name": "labels-generator",
  "version": "0.0.1",
  "description": "Manage your GitHub labels giving to your projects a wonderful, colourful and consistent labelling system",
  "main": "src/index.tsx",
  "engines": {
    "node": ">=8.11",
    "npm": ">=5.6"
  },
  "scripts": {
    "build:dev": "webpack --progress --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf dist/src dist/dev",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iarroyo5/labels-generator.git"
  },
  "keywords": [
